<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/image"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="73dp"
        android:src="@mipmap/img_deep_clean"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"/>
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="20sp"
        android:textColor="#333333"
        android:gravity="center"
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19.7dp"
        android:text="@string/guid_allow_deep_clean"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image"/>
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:overScrollMode="never"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintTop_toBottomOf="@+id/text">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:padding="18dp"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/icon_cache_sys"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="10dp"
                android:src="@mipmap/ic_item_sys_cache"
                android:scaleType="centerInside"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="12.3dp"
                app:layout_constraintBottom_toTopOf="@+id/icon_cache"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/icon_cache"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@mipmap/ic_item_cache"
                android:scaleType="centerInside"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="12.3dp"
                app:layout_constraintBottom_toTopOf="@+id/icon_residual"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/icon_cache_sys"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/icon_residual"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@mipmap/ic_item_residual"
                android:scaleType="centerInside"
                android:layout_marginEnd="12.3dp"
                app:layout_constraintBottom_toTopOf="@+id/icon_ad"
                app:layout_constraintStart_toStartOf="@+id/icon_cache"
                app:layout_constraintTop_toBottomOf="@+id/icon_cache"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/icon_ad"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@mipmap/ic_item_ad"
                android:scaleType="centerInside"
                android:layout_marginEnd="12.3dp"
                app:layout_constraintBottom_toTopOf="@+id/icon_apk"
                app:layout_constraintStart_toStartOf="@+id/icon_cache"
                app:layout_constraintTop_toBottomOf="@+id/icon_residual"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/icon_apk"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@mipmap/ic_item_apk"
                android:scaleType="centerInside"
                android:layout_marginEnd="12.3dp"
                app:layout_constraintBottom_toTopOf="@+id/icon_log"
                app:layout_constraintStart_toStartOf="@+id/icon_cache"
                app:layout_constraintTop_toBottomOf="@+id/icon_ad"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/icon_log"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@mipmap/ic_item_log"
                android:scaleType="centerInside"
                android:layout_marginEnd="12.3dp"
                app:layout_constraintBottom_toTopOf="@+id/icon_temp"
                app:layout_constraintStart_toStartOf="@+id/icon_cache"
                app:layout_constraintTop_toBottomOf="@+id/icon_apk"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/icon_temp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:src="@mipmap/ic_item_temp"
                android:scaleType="centerInside"
                android:layout_marginEnd="12.3dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/icon_cache"
                app:layout_constraintTop_toBottomOf="@+id/icon_log"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/clean_type_title7"
                android:layout_marginStart="12.3dp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_cache_sys"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon_cache_sys"
                app:layout_constraintTop_toTopOf="@+id/icon_cache_sys"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/clean_type_title1"
                android:layout_marginStart="12.3dp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_cache"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon_cache"
                app:layout_constraintTop_toTopOf="@+id/icon_cache"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/clean_type_title2"
                android:layout_marginStart="12.3dp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_residual"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon_residual"
                app:layout_constraintTop_toTopOf="@+id/icon_residual"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/clean_type_title3"
                android:layout_marginStart="12.3dp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_apk"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon_apk"
                app:layout_constraintTop_toTopOf="@+id/icon_apk"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/clean_type_title6"
                android:layout_marginStart="12.3dp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_ad"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon_ad"
                app:layout_constraintTop_toTopOf="@+id/icon_ad"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/clean_type_title5"
                android:layout_marginStart="12.3dp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_log"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon_log"
                app:layout_constraintTop_toTopOf="@+id/icon_log"/>
            <androidx.appcompat.widget.AppCompatTextView
                android:textSize="16sp"
                android:textColor="#333333"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/clean_type_title4"
                android:layout_marginStart="12.3dp"
                app:layout_constraintBottom_toBottomOf="@+id/icon_temp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/icon_temp"
                app:layout_constraintTop_toTopOf="@+id/icon_temp"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
    <androidx.appcompat.widget.AppCompatTextView
        android:textSize="17sp"
        android:textColor="@color/white"
        android:gravity="center"
        android:id="@+id/button"
        android:background="@drawable/splash_button_bg"
        android:layout_width="280dp"
        android:layout_height="43.3dp"
        android:layout_marginBottom="22.7dp"
        android:text="@string/guid_allow_ok"
        android:elevation="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:textAllCaps="true"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/close"
        android:padding="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:src="@mipmap/icon_close"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/SelectedItemBorderless"/>
</androidx.constraintlayout.widget.ConstraintLayout>